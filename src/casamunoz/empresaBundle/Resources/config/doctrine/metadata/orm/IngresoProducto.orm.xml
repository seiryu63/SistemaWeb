<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IngresoProducto" table="ingreso_producto">
    <indexes>
      <index name="IDX_CDB5E70C3B7E4BA" columns="pro_id"/>
      <index name="IDX_CDB5E7087CAE585" columns="ing_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <many-to-one field="ing" target-entity="Ingreso">
      <join-columns>
        <join-column name="ing_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="pro" target-entity="Producto">
      <join-columns>
        <join-column name="pro_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
