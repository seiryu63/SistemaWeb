<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Servicio" table="servicio">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nombreServicio" type="string" column="nombre_servicio" length="30" nullable="false"/>
    <field name="tiempoServicio" type="string" column="tiempo_servicio" length="25" nullable="false"/>
    <field name="costoServicio" type="decimal" column="costo_servicio" precision="8" scale="2" nullable="false"/>
    <field name="descripcionServicio" type="string" column="descripcion_servicio" length="100" nullable="false"/>
  </entity>
</doctrine-mapping>
