<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Municipio" table="municipio">
    <indexes>
      <index name="IDX_FE98F5E0814AA86C" columns="dep_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nombreMunicipio" type="string" column="nombre_municipio" length="30" nullable="false"/>
    <many-to-one field="dep" target-entity="Departamento">
      <join-columns>
        <join-column name="dep_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
