<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FacturaReserva" table="factura_reserva">
    <indexes>
      <index name="IDX_D86DE6E4670E604" columns="res_id"/>
      <index name="IDX_D86DE6E7BAE57D6" columns="fac_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <many-to-one field="fac" target-entity="Factura">
      <join-columns>
        <join-column name="fac_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="res" target-entity="Reserva">
      <join-columns>
        <join-column name="res_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
