<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Producto" table="producto">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nombreProducto" type="string" column="nombre_producto" length="50" nullable="false"/>
    <field name="existencia" type="integer" column="existencia" nullable="false"/>
    <field name="descripcionProducto" type="string" column="descripcion_producto" length="100" nullable="false"/>
  </entity>
</doctrine-mapping>
