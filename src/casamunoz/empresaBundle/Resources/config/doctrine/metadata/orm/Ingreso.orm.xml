<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ingreso" table="ingreso">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nombreProducto" type="string" column="nombre_producto" length="50" nullable="false"/>
    <field name="cantidad" type="integer" column="cantidad" nullable="false"/>
    <field name="costoUnitario" type="decimal" column="costo_unitario" precision="8" scale="2" nullable="false"/>
    <field name="costoTotal" type="decimal" column="costo_total" precision="8" scale="2" nullable="false"/>
  </entity>
</doctrine-mapping>
