{% extends 'casamunozempresaBundle::plantilla.html.twig' %}

{% block body -%}
    <h1>Sucursal</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombresucursal</th>
                <td>{{ entity.nombreSucursal }}</td>
            </tr>
            <tr>
                <th>Ubicacion</th>
                <td>{{ entity.ubicacion }}</td>
            </tr>
            <tr>
                <th>Descripcionsucursal</th>
                <td>{{ entity.descripcionSucursal }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('sucursal') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('sucursal_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
